<h1>New Artist Form</h1>

<%= form_tag artists_path do %>
<p>
  <%= label_tag :'artist[name]', "Artist Name:" %>
  <%= text_field_tag :'artist[name]' %>
</p>

<p>
  <%= label_tag :'artist[name]', "Artist Bio:" %>
  <%= text_area_tag :'artist[bio]' %>
</p>

<%= submit_tag "Create Artist" %>
<% end %>
